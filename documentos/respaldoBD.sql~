  CONSTRAINT gerencias_pkey PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE gerencias
  OWNER TO inventario;
COMMENT ON TABLE gerencias
  IS 'Tabla catálogo de gerencias';

